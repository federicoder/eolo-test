<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Invito.
    -->
    <changeSet id="20200506145100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="collaboratore_id"
                                 baseTableName="invito"
                                 constraintName="fk_invito_collaboratore_id"
                                 referencedColumnNames="id"
                                 referencedTableName="collaboratore"/>

        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="invito"
                                 constraintName="fk_invito_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

        <addForeignKeyConstraint baseColumnNames="pratica_id"
                                 baseTableName="invito"
                                 constraintName="fk_invito_pratica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pratica"/>

        <addForeignKeyConstraint baseColumnNames="professionista_id"
                                 baseTableName="invito"
                                 constraintName="fk_invito_professionista_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professionista"/>

    </changeSet>
</databaseChangeLog>
